{
	"manifest_version": 2,
	"name": "Tws",
	"description": "Scrape data from twitter search manually",
	"homepage_url": "https://github.com/mohamedarish/tws",
	"version": "1.0",
	"background": {
		"scripts": [
			"./dist/background.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"https://twitter.com/search?*q=*"
			],
			"js": [
				"./dist/content.js"
			]
		}
	],
	"permissions": [
		"menus",
		"activeTab",
		"scripting"
	]
}
